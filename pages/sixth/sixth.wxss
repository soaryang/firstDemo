.filmContent {
    margin-top: 20rpx;
    
    width:100%;
}

.filmContent .filmTitleview {
   margin-left:20rpx; 
   margin-bottom: 20rpx;
}

.filmContent .filmTitleview .tileText{
    font-size: 12pt;
}

.filmContent .filmListView {
    display:flex;flex-direction:column;width:94%;height:100%; padding-left:10px;margin-right:10px;
}

.filmContent .filmListView .scrollView {
    white-space:nowrap; 
    display: block;
}

.filmContent .filmListView .scrollView .filePictureView{
    display: inline-block;
    margin-right: 25rpx;
}


.gameContent {
    margin-top: 20rpx;
    width:100%;
    border-bottom: 10rpx solid lightgray;
}

.gameContent .gameTitleview {
   margin-left:20rpx; 
   margin-bottom: 20rpx;
}

.gameContent .gameTitleview .tileText{
    font-size: 12pt;
}

.gameContent .gameListView {
    width:100%;
    display:flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
}

.gameContent .gameListView  .big-photo-item{
    text-align: center;
    width:280rpx;
    display: flex;
    flex-direction: column;
    padding:5rpx 0rpx;
}

.gameContent .gameListView  .big-photo-item .photo{
    width:100%;
    height: 140rpx;
    border-radius: 10rpx;
    background: #aaaaaa;
}

.gameContent .gameListView  .big-photo-item .gameName{
    margin-top: 10rpx;
    font-size: 9pt;
}

