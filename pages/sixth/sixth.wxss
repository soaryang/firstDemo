
.imageContent{
   /*padding-bottom: 10px;
   background-color: lightgray;*/
   padding-bottom: 12rpx;
   border-bottom: 10rpx solid lightgray;
}

.imageContent .imageView{
    display:flex;
    flex-direction:row;
    flex-wrap: wrap;
    justify-content: center;
    
}

.imageContent .imageView .imageViewList{
    flex:0;
	  margin-bottom: -5px;
}

.imageContent .imageView .imageViewList .dialogView{
    margin-left:100rpx;
    padding-bottom:0px;
    margin-top:30rpx;
    max-width: 75px;
    max-height: 30px;
    font-size: 8pt;
    position:absolute;
    background-color: white;
    color: black;
    border:0.5px solid #ccc;
    text-align: left;
    border-radius: 2px;
    font-family: sans-serif;
    z-index: 1000;
}

.speech-bubble {
    position: relative;
    color: white;
    text-align: center;
    border-radius: 0px;
    font-family: sans-serif;
}

  .speech-bubble:after {
    content: '';
    position: absolute;
    width: 0;
    height: 0;
    border: 7px solid;
    border-color:transparent ;
}

.speech-bubble-left:after {
    border-style:solid;
    border-right-color: white;
    top: 50%;
    right: 100%;
    margin-top: -8.5px;
}

.imageView .imageViewList .imageCss{
    border:1px solid white;
    width: 140rpx;
    height: 140rpx;
    /*border-radius: 50%;*/
}

.imageView2{
    position: absolute;
    margin-top:110rpx;
    z-index: 2000;
}

.imageView2 .imageCss2{
    border:2px solid gold;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
}


